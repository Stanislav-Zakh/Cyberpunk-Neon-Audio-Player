<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <LinearGradientBrush x:Key="DataBrush"  StartPoint="0 0" EndPoint="1 0">
        <GradientStop Color="#CCC856" Offset="0"/>
        <GradientStop Color="#CCC856" Offset="1.0"/>
    </LinearGradientBrush>


    <RadialGradientBrush x:Key="StormyNightBrush" SpreadMethod="Repeat"  RadiusX="5" RadiusY="2"  GradientOrigin="0.5 1.0">

        <GradientStop Color="#883c3ba3" Offset="0"/>

        <GradientStop Color="#8809143C" Offset="0.12"/>


        <GradientStop Color="#8809143C" Offset="1.0"/>

    </RadialGradientBrush>




</ResourceDictionary>